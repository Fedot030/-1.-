#include <stdio.h>
int main()
{
  int i,j,n,p,k;
 k=1;
  //задание 2 множеств
  int a[10];
  int arr[10];
  //ввод элементов массива
  printf("Введите n: ");
  scanf("%d",&n);
  for(i=0;i<n;i++)
  {
      scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++)
              {
                  printf("%d",a[i]);
              }
  
 
  for(i=0;i<n;i++)
      {
          for(int j=0;j<n;j++)
          {
          if(a[i]==a[j])
          {
              k=0;
              break;
          }
          }
      }
      if(k==1)
     {
                 printf("\n");
             //удаление элементов массива
            
            for(i=n;i>0;i--)
            {
                a[i]=a[i-1];
            }
             //добавление элементов множества
            scanf("%d",&p);
            i=n++;
            a[n]=p;
      }
      else
      {
          printf("Ne verno vvedeno mnojestvo");
      }
      return 0;
}

